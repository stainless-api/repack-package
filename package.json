{
  "name": "@stainless-api/repack-package",
  "version": "0.0.1",
  "description": "Repack NPM tgz files for better compression.",
  "private": false,
  "bin": {
    "repack": "bin/repack.mjs"
  },
  "scripts": {
    "prepack": "pnpm check",
    "check": "tsc && prettier --check '**/*'.{mjs,json}",
    "fmt": "prettier --write '**/*'.{mjs,json}",
    "prepublish": "echo 'run ./scripts/publish.sh'; exit 1",
    "test": "./scripts/test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stainless-api/repack-package.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stainless-api/repack-package/issues"
  },
  "homepage": "https://github.com/stainless-api/repack-package#readme",
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "dependencies": {
    "pretty-bytes": "^6.1.1",
    "pretty-ms": "^9.2.0",
    "yocto-spinner": "^0.1.2",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
}
